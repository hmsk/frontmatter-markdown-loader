{
  "name": "frontmatter-markdown-loader",
  "version": "1.5.1",
  "description": "Webpack loader for Front Matter-ed Markdown file to get frontmatter attributes and compiled Vue component",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hmsk/frontmatter-markdown-loader.git"
  },
  "keywords": [
    "webpack",
    "markdown",
    "frontmatter",
    "vue",
    "loader"
  ],
  "author": "Kengo Hamasaki <k.hamasaki@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hmsk/frontmatter-markdown-loader/issues"
  },
  "homepage": "https://github.com/hmsk/frontmatter-markdown-loader#readme",
  "dependencies": {
    "front-matter": "^3.0.1",
    "loader-utils": "^1.1.0",
    "markdown-it": "^8.4.2"
  },
  "optionalDependencies": {
    "vue-template-compiler": "^2.5.21",
    "vue-template-es2015-compiler": "^1.6.0"
  },
  "devDependencies": {
    "@vue/test-utils": "1.0.0-beta.28",
    "babel-preset-env": "1.7.0",
    "jest": "23.6.0",
    "node-eval": "2.0.0",
    "vue": "2.5.21",
    "vue-jest": "3.0.2",
    "vue-template-compiler": "2.5.21",
    "vue-template-es2015-compiler": "1.6.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    }
  }
}
